<div class="container">
    <h1 class="text-center">Meus Frillas</h1>
    <div class="row">
        <div class="col-md-8 col-md-offset-2">

            <div class="overview">
                <div class="add-frilla-card">
                    <p class="text-center" style="margin-top: 40px;">
                        <a href="/add-novo-frilla" class="btn btn-success btn-lg">Adicionar novo Frilla</a>
                </div>
            </div>

            <div class="overview">
                <div class="list-orders-card">
                    <div class="table-margin">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th>Frilla</th>
                                    <th>Titulo</th>
                                    <th>Sobre</th>
                                    <th>R$</th>
                                </tr>
                            </thead>
                            <tbody>
                                {{#each frillas}}
                                <tr>
                                    <td><img class="order-image-my-frilla" src="{{foto}}"></td>
                                    <td>{{titulo}}</td>
                                    <td>{{sobre}}</td>
                                    <td>{{preco}}</td>
                                </tr>
                                {{/each}}
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>

        </div>
    </div>
</div>